#, no-wrap
msgid ""
"cd ~/code\n"
"git clone https://gitlab.com/fdroid/fdroidserver.git\n"
"git clone https://gitlab.com/fdroid/fdroiddata.git\n"
"echo 'export PATH=\"~/code/fdroidserver:$PATH\"' >> ~/.profile source ~/.profile\n"
